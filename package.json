{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "Gatsby example site with Cypress E2E tests and code coverage",
  "version": "0.1.0",
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "cypress test",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "merge-reports": "jrm results/combined.xml \"results/*.xml\"",
    "dev": "NODE_ENV=develop start-test develop 8000 cy:run"
  },
  "dependencies": {
    "@uipath/apollo-material-ui-theme": "^0.20.0",
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
